<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Upload your surfboard details</h1>
      <%= simple_form_for @surfboard do |f| %>
        <%= f.input :photo, as: :file%>
        <%= f.input :city %>
        <%= f.input :price_day %>
        <%= f.input :brand %>
        <%= f.input :detail %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
